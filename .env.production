# 生产环境配置文件
# 只在生产环境加载

# 环境标识
NODE_ENV=production

# 是否开启调试模式
VITE_APP_DEBUG=false

# API基础路径 - 生产环境通常指向实际API服务器
VITE_API_BASE_URL=/api

# API服务器地址 - 生产环境
VITE_API_SERVER=http://163.22.55.188

# 是否启用Vue Devtools
VITE_USE_DEVTOOLS=false

# 是否启用源码映射
VITE_USE_SOURCEMAP=false

# 是否启用代码压缩
VITE_USE_COMPRESS=true

# 是否启用gzip压缩
VITE_USE_GZIP=true